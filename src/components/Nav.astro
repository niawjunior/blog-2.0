---
interface Props {
  menus: {
    label: string
    link: string
  }[]
}

const { menus } = Astro.props
---

<div class="flex bg-blue-400 px-4 py-4 gap-4 mb-10">
  {
    menus?.map((item) => {
      return (
        <a href={item.link} class="font-bold text-white">
          {item.label}
        </a>
      )
    })
  }
</div>
<style></style>
